{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Outline",
  "scopeName": "text.outline",
  "patterns": [
    {
      "name": "comment.line.number-sign.outline",
      "match": "^\\s*#.*"
    },
    {
      "name": "keyword.control.outline",
      "match": "^# use .*"
    }
  ]
}