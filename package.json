{
  "name": "design-resolve",
  "displayName": "design-resolve",
  "description": "",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "letWindTellUrMom",
  "repository": {
    "url": "https://github.com/Tramis/design-resolve"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "design-resolve.helloWorld",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "id": "design",
        "aliases": [
          "Design",
          "design"
        ],
        "extensions": [
          ".des"
        ],
        "configuration": "./design-language-configuration.json"
      },
      {
        "id": "outline",
        "aliases": [
          "Outline",
          "outline"
        ],
        "extensions": [
          ".outline"
        ],
        "configuration": "./outline-language-configuration.json"
      }
    ],
    "iconThemes": [
      {
        "id": "design-resolve-icons",
        "label": "Your Extension Name Icons",
        "path": "./file-icons.json"
      }
    ],
    "grammars": [
      {
        "language": "design",
        "scopeName": "text.design",
        "path": "./syntaxes/design.tmLanguage.json"
      },
      {
        "language": "outline",
        "scopeName": "text.outline",
        "path": "./syntaxes/outline.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}